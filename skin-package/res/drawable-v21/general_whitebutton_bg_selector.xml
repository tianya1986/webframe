<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
        android:color="@color/general_whitebutton_bg_ripple">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/general_whitebutton_color_selector"/>
            <corners android:radius="4dp"/>
            <padding
                android:bottom="8dp"
                android:left="16dp"
                android:right="16dp"
                android:top="8dp"/>
            <stroke
                android:width="0.5dp"
                android:color="@color/general_whitebutton_stroke"/>
        </shape>
    </item>
</ripple>